{
    "name": "phpstan\/phpstan-compiler",
    "type": "project",
    "description": "PHAR Compiler for PHPStan",
    "license": [
        "MIT"
    ],
    "require": {
        "php": "^7.3",
        "nette\/neon": "^3.0.0",
        "symfony\/console": "^5.2.2",
        "symfony\/process": "^5.2.2",
        "symfony\/filesystem": "^5.2.2",
        "symfony\/finder": "^5.2.2"
    },
    "autoload": {
        "psr-4": {
            "TenantCloud\\BetterReflection\\Relocated\\PHPStan\\Compiler\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TenantCloud\\BetterReflection\\Relocated\\PHPStan\\Compiler\\": "tests\/"
        }
    },
    "require-dev": {
        "phpunit\/phpunit": "^9.5.1",
        "phpstan\/phpstan-phpunit": "^0.12.8"
    },
    "config": {
        "platform": {
            "php": "7.3.24"
        },
        "platform-check": false,
        "sort-packages": true
    }
}