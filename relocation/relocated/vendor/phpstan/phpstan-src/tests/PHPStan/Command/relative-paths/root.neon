parameters:
	bootstrap: here.php
	bootstrapFiles:
		- here.php

	autoload_files:
		- here.php
		- test/there.php
		- ../up.php

	autoload_directories:
		- src
		- .
		- %rootDir%/conf

	scanFiles:
		- here.php
		- test/there.php
		- ../up.php

	scanDirectories:
		- src
		- .
		- %rootDir%/conf

	paths:
		- src

	memoryLimitFile: .memory_limit
	excludes_analyse:
		- src
		- src/*/data
		- */src/*/data

